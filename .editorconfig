# http://editorconfig.org
root = true

[*]

# Change these settings to your own preference
indent_size = 2
indent_style = tab

# We recommend you to keep these unchanged
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[Makefile]
indent_style = tab
